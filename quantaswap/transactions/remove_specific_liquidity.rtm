CALL_METHOD 
    Address("${ACCOUNT}") 
    "withdraw_non_fungibles" 
    Address("${LIQUIDITY_RECEIPT}")
    Array<NonFungibleLocalId>(NonFungibleLocalId("${LIQUIDITY_RECEIPT_ID}"))
;
TAKE_ALL_FROM_WORKTOP 
    Address("${LIQUIDITY_RECEIPT}") 
    Bucket("receipt")
;

